# Test Environment Variables für CI/CD
# Diese Datei wird in GitHub Actions verwendet

# API-Konfiguration
APP_NAME=Titanic Passagier API (Test)
VERSION=1.0.0
DEBUG=true

# Server-Konfiguration
HOST=0.0.0.0
PORT=8000

# Test-Datenbank-Konfiguration (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=titanic
DB_USER=postgres
DB_PASSWORD=${TEST_DB_PASSWORD}

# Logging
LOG_LEVEL=INFO

# API-Limits
MAX_PAGE_SIZE=500
DEFAULT_PAGE_SIZE=50

# CORS (für Tests offen)
CORS_ORIGINS=["*"]

# Security (für Tests offen)
ALLOWED_HOSTS=["*"]
